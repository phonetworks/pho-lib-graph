{
  "name": "phonetworks/pho-lib-graph",
  "type": "library",
  "description": "A general purpose graph library in PHP",
   "keywords": [
        "graph",
        "network",
        "node",
        "vertex",
        "edge",
	"predicate"
    ],
  "homepage": "https://phonetworks.org",
  "license": "MIT",
  "authors": [
        {
            "name": "Emre Sokullu",
            "email": "emre@phonetworks.org",
            "homepage": "http://www.emresokullu.com"
        }
    ],
    "require": {
        "php":">=5.3.0",
        "psr/log": "^1.0",
        "sabre/event": "^5.0",
        "opis/closure": "^3.0.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
	    "phpdocumentor/phpdocumentor": "*" ,
        "psy/psysh": "*",
        "fzaninotto/faker": "^1.6",
        "mockery/mockery": "dev-master",
	    "squizlabs/php_codesniffer": "3.*",
	    "roave/security-advisories": "dev-master"
        },
    "autoload": {
	     "psr-4": {
		       "Pho\\Lib\\Graph\\": "src/Pho/Lib/Graph/"
	     }
    },
    "autoload-dev": {
	     "psr-4": {
		       "Pho\\Lib\\Graph\\": "tests/Pho/Lib/Graph/"
	     }
    }
}
